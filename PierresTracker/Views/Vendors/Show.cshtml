@using PierresTracker.Models;
<h3>Here are all the orders in this vendor:</h3>
<ol>
@foreach (Order order in @Model["orders"])
{
  <li><a href='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.OrderDetails</a></li>
}
</ol>
<p><a href='/vendors/@Model["vendor"].Id/orders/new'>Add a new order</a></p>
<p><a href='/vendors'>Vendor List</a></p>
<p><a href='/'>Home</a></p> 